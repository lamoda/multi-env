{
  "name": "lamoda/multi-env",
  "description": "Library that provides classes to work with envs in multitenant environment",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ulrichsg/getopt-php": "^4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~v3.12.0",
    "phpunit/phpunit": "^9.5",
    "vlucas/phpdotenv": "v3.6.10"
  },
  "suggest": {
    "vlucas/phpdotenv": "Required for DotEnvFileReaderAdapter"
  },
  "autoload": {
    "psr-4": {
      "Lamoda\\MultiEnv\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lamoda\\MultiEnvTests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
