{
  "name": "lamoda/multi-env",
  "description": "Library that provides classes to work with envs in multitenant environment",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ulrichsg/getopt-php": "^3.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "phpunit/phpunit": "^7.5",
    "vlucas/phpdotenv": "^2.4"
  },
  "suggest": {
    "vlucas/phpdotenv": "Required for DotEnvV2FileReaderAdapter."
  },
  "autoload": {
    "psr-4": {
      "Lamoda\\MultiEnv\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lamoda\\MultiEnvTests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
